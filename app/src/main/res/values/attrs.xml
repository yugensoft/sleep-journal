<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SleepComparisonBar">
        <attr name="horizontalBarColor" format="color"/>
        <attr name="horizontalBarBorder" format="dimension"/>
        <attr name="horizontalBarBorderColor" format="color"/>
        <attr name="horizontalBarThickness" format="dimension"/>
        <attr name="textToBarPadding" format="dimension"/>
        <attr name="labelColor" format="color"/>
        <attr name="labelPosition" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
        <attr name="labelWidth" format="dimension"/>
        <attr name="labelHeight" format="dimension"/>
    </declare-styleable>

</resources>